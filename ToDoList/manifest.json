{
    "manifest_version": 3,
    "name": "To-Do List Page",
    "version": "1",
    "description": "Remplace la page d'accueil par une application de to-do list.",
    "permissions": ["storage"],
    "chrome_url_overrides": {
      "newtab": "popup.html"
    },
    "background": {
      "service_worker": "background.js"
    },
    "action": {
        "default_icon": "house.png",
      "default_popup": "popup.html"
    }
      
    }
    
    


  